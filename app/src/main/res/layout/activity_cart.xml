<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_grey"
    tools:context=".CartActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Total:"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCheckOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:backgroundTint="@color/yellow"
        android:text="Pay"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="112dp"
        android:text="E-Wallet"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cashs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="112dp"
        android:text="Cash"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/debit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="112dp"
        android:text="Debit Card"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="parent" />

    <ListView
        android:id="@+id/cartListView"
        android:layout_width="402dp"
        android:layout_height="300dp"
        android:layout_marginTop="136dp"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/buttonCheckOut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="291dp"
        android:text="Payment Method"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cartListView" />

    <TextView
        android:id="@+id/cashbtn"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_marginBottom="136dp"
        android:background="@drawable/radius_custom"
        android:translationZ="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/debitbtn"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/debitbtn"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_marginBottom="136dp"
        android:background="@drawable/radius_custom"
        android:translationZ="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/walletbtn"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/radius_custom"

        android:drawablePadding="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.28" />

    <ImageView
        android:id="@+id/walletImageView"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:src="@drawable/wallet_svgrepo_com"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/walletbtn"
        app:layout_constraintEnd_toEndOf="@+id/walletbtn"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toStartOf="@+id/walletbtn"
        app:layout_constraintTop_toTopOf="@+id/walletbtn" />

    <ImageView
        android:id="@+id/debitImage"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/debit_card_svgrepo_com"
        android:translationZ="2dp"
        app:layout_constraintBottom_toTopOf="@+id/debit"
        app:layout_constraintEnd_toEndOf="@+id/debitbtn"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="@+id/debitbtn"
        app:layout_constraintTop_toTopOf="@+id/debitbtn"
        app:layout_constraintVertical_bias="0.571"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/cashImage"
        android:layout_width="55dp"
        android:layout_height="48dp"
        android:src="@drawable/cash_dispenser_dollar_svgrepo_com"
        android:translationZ="2dp"
        app:layout_constraintBottom_toTopOf="@+id/cashs"
        app:layout_constraintEnd_toEndOf="@+id/cashbtn"
        app:layout_constraintStart_toStartOf="@+id/cashbtn"
        app:layout_constraintTop_toTopOf="@+id/cashbtn"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>